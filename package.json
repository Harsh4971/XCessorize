{
  "name": "pose-babylon",
  "version": "0.6.5",
  "private": true,
  "title": "Pose Tracking Demo",
  "description": "Geenee pose tracking demo",
  "dependencies": {
    "@babylonjs/core": "^6.41.0",
    "@babylonjs/loaders": "^6.41.0",
    "@geenee/armature": "0.6.8",
    "@geenee/bodyprocessors": "0.6.8",
    "@geenee/bodyrenderers-babylon": "0.6.8",
    "@tensorflow/tfjs-converter": "^4.20.0",
    "@tensorflow/tfjs-core": "^4.20.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.14",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "esbuild-loader": "^4.0.2",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.6.1",
    "serve": "^14.2.1",
    "typescript": "^5.5.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "start": "webpack serve --mode=development",
    "start:https": "webpack serve --mode=development --https",
    "build": "webpack build --mode=production",
    "serve": "serve -p 3000 -s build",
    "postinstall": "npm run post:wasm",
    "post:wasm": "copyfiles -f \"node_modules/@geenee/bodytracking/dist/*.wasm\" public/"
